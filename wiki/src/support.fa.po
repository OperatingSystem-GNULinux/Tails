#, no-wrap
msgid "<div id=\"talk\">\n"
msgstr "<div id=\"talk\">\n"

#. type: Plain text
#, no-wrap
msgid "  <h1>Get in touch with us</h1>\n"
msgstr "  <h1>با ما تماس بگیرید</h1>\n"

#. type: Plain text
#, no-wrap
msgid "  [[!inline pages=\"support/talk\" raw=\"yes\"]]\n"
msgstr "  [[!inline pages=\"support/talk\" raw=\"yes\"]]\n"

