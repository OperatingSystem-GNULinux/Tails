msgstr ""

#. type: Plain text
#, no-wrap
msgid "<p class=\"start\">Start in Mac OS X.</p>\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "[[!inline pages=\"install/inc/tails-installation-assistant.inline\" raw=\"yes\"]]\n"
msgstr ""

#. type: Plain text
